# specify spectral template for SNIa
SN_SED: $SNDATA_ROOT/snsed/Hsiao07.dat
BD17_SED:    $PS1_USERS/dscolnic/Excalibur_dillon/calspec/bd_17d4708_stisnic_007.dat

# specify filters :  
MAGSYSTEM: AB
FILTSYSTEM: COUNT
FILTPATH:   /project2/rkessler/PRODUCTS/SNDATA_ROOT/filters/ATLAS
SURVEY: ATLAS
FILTER:  ATLAS-c   atlas-c.txt   0.0
